{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "semi": 2,
    "no-var": 2,
    "indent": 2,
    "no-multi-spaces": 2,
    "space-in-parens": 2,
    "no-multiple-empty-lines": 2,
    "prefer-const": 2,
    "no-use-before-define": 2
  }
}
